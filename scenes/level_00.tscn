[sub_resource type="GDScript" id="GDScript_cr2eh"]
script/source = "extends AnimatableBody3D

@onready var label_3d := $button/Label3D

@export_enum(\"lift\", \"platform\") var animation_name: String = \"lift\"

@export var open := false :
	set(value):
		if value != open:
			open = value
			update_door()
	
func update_door() -> void:
	if !$AnimationPlayer.has_animation(animation_name):
		print(\"Warning: Animation '\" + animation_name + \"' not found!\")
		return

	if open:
		$AnimationPlayer.play(animation_name)
	else:
		$AnimationPlayer.play_backwards(animation_name)
	$AnimationPlayer.set_active(true)
	
func toggle_open() -> void:
	open = !open

func _process(_delta) -> void:
	label_3d.visible = !!$InteractableComponent.get_character_hovered_by_cur_camera()
"

