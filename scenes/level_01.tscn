[sub_resource type="GDScript" id="GDScript_cr2eh"]
script/source = "extends AnimatableBody3D

@export var open := false :
	set(value):
		if value != open:
			open = value
			update_door()
	
func update_door():
	if open:
		$AnimationPlayer.play(\"lift\")
	else:
		$AnimationPlayer.play_backwards(\"lift\")
	$AnimationPlayer.set_active(true)
	
func toggle_open():
	open = !open
"

